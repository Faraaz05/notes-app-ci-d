{
  "name": "notes-app-backend",
  "version": "1.0.0",
  "description": "Backend for modern note-taking app",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules --localstorage-file=./jest-storage' jest --runInBand",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules --localstorage-file=./jest-storage' jest --watch --runInBand",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules --localstorage-file=./jest-storage' jest --coverage --runInBand"
